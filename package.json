{
	"name": "first-test",
	"displayName": "first-test",
	"description": "just for test extenstion workflow",
	"version": "0.0.5",
	"publisher": "financialbdscan",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/gejun2008/bd-fix.git"
	  },
	"activationEvents": [
		"onCommand:first-test.bd-fix",
		"onCommand:first-test.bd-scan"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "first-test.bd-fix",
				"title": "BD Fix"
			},
			{
				"command": "first-test.bd-scan",
				"title": "BD Scan"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"ts-loader": "^9.2.6",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.2"
	}
}
